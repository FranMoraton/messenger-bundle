{
  "name": "pccomponentes/messenger-bundle",
  "description": "Symfony Messenger Bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Zoilo Mora",
      "email": "zoilo.mora@pccomponentes.com"
    },
    {
      "name": "Aar√≥n Bernabeu",
      "email": "aaron.bernabeu@pccomponentes.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PcComponentes\\SymfonyMessengerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PcComponentes\\SymfonyMessengerBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "ext-amqp": "*",
    "php": "^7.4 || ^8.0",
    "pccomponentes/ddd-logging": "^2.1",
    "pccomponentes/ddd": "^2.0 || ^3.0",
    "beberlei/assert": "^3.2",
    "ramsey/uuid": "^4.0"
  },
  "require-dev": {
    "pccomponentes/coding-standard": "^1.0",
    "phpunit/phpunit": "^9"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
